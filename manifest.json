{   
 "manifest_version": 2,
  "name": "Heed",
  "description": "Focus Help Web Extension",
  "version": "1.0.0",
  "icons": {"128": "HeedIcon_128.png"},
  "browser_action": {
      "default_icon": "HeedIcon.png",
      "default_popup": "popup.html"
  },
  "permissions": ["activeTab","storage", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }    
}